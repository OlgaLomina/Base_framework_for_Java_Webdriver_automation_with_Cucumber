{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "totalScore": {
      "type": "integer"
    },
    "passingPercentage": {
      "type": "integer"
    },
    "showStopperQuestion": {
      "type": "null"
    },
    "questions": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            },
            "question": {
              "type": "string"
            },
            "score": {
              "type": "integer"
            }
          },
          "required": [
            "type",
            "question",
            "score"
          ]
        }
      ]
    },
    "createdAt": {
      "type": "null"
    },
    "updatedAt": {
      "type": "null"
    }
  },
  "required": [
    "id",
    "name",
    "totalScore",
    "passingPercentage",
    "showStopperQuestion",
    "questions",
    "createdAt",
    "updatedAt"
  ]
}